{
  "name": "firebase-admin-auth",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc --build && tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "deploy": "yarn build && bash deploy.sh",
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register src/server.ts",
    "start": "NODE_ENV=production node ./dist/server.js",
    "format": "prettier --write **/*.{js,jsx,ts,tsx,json}"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/csrf": "^3.1.0",
    "@types/csurf": "^1.11.2",
    "@types/dotenv": "^8.2.0",
    "@types/express-session": "^1.17.5",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.8.3",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csrf": "^3.1.0",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.3",
    "firebase-admin": "^11.2.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "uid": "^2.0.1",
    "uuid": "^9.0.0"
  }
}
